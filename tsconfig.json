{
	"compilerOptions": {
		"target": "es5",

		"lib": ["es2015"],

		"skipLibCheck": true,
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"allowJs": true,
		// "noEmit": true,
		"outDir": "./dist"
	},
	"exclude": [
        "node_modules"
    ],
	"include": ["api/**/*", "handler.ts"]
}
